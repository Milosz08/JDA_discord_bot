{
    "botVersion": "1.0-SNAPSHOT",
    "showFancyTitle": true,
    "developmentMode": true,
    "authorization": {
        "token": "${DEV_TOKEN}",
        "applicationId": "${DEV_APPLICATION_ID}"
    },
    "executors": {
        "defPrefix": "?",
        "queuePaginationMaxElmsOnPage": 20
    },
    "sequencers": {
        "maxInactivityTimeMinutes": 1,
        "maxInactivityTimeAfterPauseTrackMinutes": 1,
        "maxVotingElapseTimeMinutes": 1,
        "activityStatusSequencer": {
            "enableRandomizeActivityStatus": true,
            "intervalSeconds": 10
        }
    },
    "miscellaneous": {
        "logger": {
            "loggerEnabled": true,
            "loggerOutputFolderName": "dev-logs",
            "loggerSensitivity": [
                "INFO", "WARN", "ERROR", "DEBUG"
            ],
            "enableLoggedToStandardOutput": true,
            "enableLoggedToFileOutput": true
        },
        "database": {
            "databaseUrl": "${DEV_DATABASE_CONNECTION_STRING}",
            "enforceSslConnection": false,
            "username": "${DEV_DATABASE_USERNAME}",
            "password": "${DEV_DATABASE_PASSWORD}",
            "createDatabaseIfNotExist": true,
            "sqlOnStandardOutput": true,
            "hibernateDriverPackage": "com.mysql.cj.jdbc.Driver",
            "hibernateDialectPackage": "org.hibernate.dialect.MySQLDialect",
            "hbm2ddlAutoMode": "none"
        }
    }
}
