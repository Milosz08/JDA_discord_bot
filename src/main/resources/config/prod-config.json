{
    "botVersion": "1.0",
    "showFancyTitle": true,
    "developmentMode": false,
    "authorization": {
        "token": "${PROD_TOKEN}",
        "applicationId": "${PROD_APPLICATION_ID}"
    },
    "executors": {
        "defPrefix": "$",
        "queuePaginationMaxElmsOnPage": 20
    },
    "sequencers": {
        "maxInactivityTimeMinutes": 5,
        "maxInactivityTimeAfterPauseTrackMinutes": 10,
        "maxVotingElapseTimeMinutes": 2,
        "activityStatusSequencer": {
            "enableRandomizeActivityStatus": true,
            "intervalSeconds": 20
        }
    },
    "miscellaneous": {
        "logger": {
            "loggerEnabled": true,
            "loggerOutputFolderName": "prod-logs",
            "loggerSensitivity": [
                "INFO", "WARN", "ERROR"
            ],
            "enableLoggedToStandardOutput": true,
            "enableLoggedToFileOutput": true
        },
        "database": {
            "databaseUrl": "${PROD_DATABASE_CONNECTION_STRING}",
            "enforceSslConnection": true,
            "username": "${PROD_DATABASE_USERNAME}",
            "password": "${PROD_DATABASE_PASSWORD}",
            "createDatabaseIfNotExist": true,
            "sqlOnStandardOutput": false,
            "hibernateDriverPackage": "com.mysql.cj.jdbc.Driver",
            "hibernateDialectPackage": "org.hibernate.dialect.MySQLDialect",
            "hbm2ddlAutoMode": "none"
        }
    }
}
