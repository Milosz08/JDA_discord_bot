{
    "botVersion": "1.0-SNAPSHOT",
    "showFancyTitle": true,
    "developmentMode": true,
    "authorization": {
        "token": "xxxx",
        "applicationId": "xxxx"
    },
    "executors": {
        "defPrefix": "?",
        "queuePaginationMaxElmsOnPage": 20
    },
    "sequencers": {
        "maxInactivityTimeMinutes": 5,
        "maxInactivityTimeAfterPauseTrackMinutes": 10,
        "maxVotingElapseTimeMinutes": 2,
        "activityStatusSequencer": {
            "enableRandomizeActivityStatus": true,
            "intervalSeconds": 10
        }
    },
    "miscellaneous": {
        "channelLogger": {
            "loggerEnabled": true,
            "loggerChannelsCategoryName": "━━━━━ LOGGER ━━━━━",
            "loggerChannels": [
                {
                    "channelType": "MOD_LOGGER",
                    "channelName": "log_mod",
                    "enabled": true
                },
                {
                    "channelType": "CHAT_LOGGER",
                    "channelName": "log_chat",
                    "enabled": true
                },
                {
                    "channelType": "VOICE_LOGGER",
                    "channelName": "log_voice",
                    "enabled": true
                }
            ]
        },
        "logger": {
            "loggerEnabled": true,
            "loggerOutputFolderName": "dev-logs",
            "loggerSensitivity": [
                "INFO", "WARN", "ERROR", "DEBUG"
            ],
            "enableLoggedToStandardOutput": true,
            "enableLoggedToFileOutput": true
        },
        "database": {
            "databaseUrl": "jdbc:[dbType]://[dbServer]:[dbPort]/[dbName]",
            "enforceSslConnection": true,
            "username": "xxxx",
            "password": "xxxx",
            "createDatabaseIfNotExist": true,
            "sqlOnStandardOutput": true,
            "hibernateDriverPackage": "[external database driver package, ex. com.mysql.cj.jdbc.Driver]",
            "hibernateDialectPackage": "org.hibernate.dialect.[selected database dialect]",
            "hbm2ddlAutoMode": "validate"
        }
    }
}